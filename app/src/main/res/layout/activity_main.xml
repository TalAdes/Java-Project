<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@drawable/akasaka_minato_japan_night_city_119095_1440x2560"
    android:orientation="vertical"
    tools:context="com.example.liran.takeogo.controller.MainActivity">

    <!--finish buttons config-->

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="-20dp"
        android:orientation="horizontal"
        app:layout_constraintBottom_toBottomOf="parent"
        tools:layout_editor_absoluteX="0dp">

        <ImageView
            android:id="@+id/imageView10"
            android:layout_width="295dp"
            android:layout_height="wrap_content"
            android:layout_marginLeft="80dp"
            android:layout_marginTop="50dp"
            android:layout_weight="1"
            android:background="#00000000"
            android:src="@drawable/takego"
            tools:layout_editor_absoluteX="100dp"
            tools:layout_editor_absoluteY="2dp" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="105dp">

        <ImageButton
            android:id="@+id/buttonClient"
            android:layout_width="122dp"
            android:layout_height="113dp"
            android:layout_weight="1"
            android:background="#00000000"
            android:src="@drawable/imageclient"
            tools:layout_editor_absoluteX="196dp"
            tools:layout_editor_absoluteY="93dp" />

        <ImageButton
            android:id="@+id/buttonbrunches"
            android:layout_width="93dp"
            android:layout_height="120dp"
            android:layout_weight="1"
            android:background="#00000000"
            android:src="@drawable/home_icon"
            tools:layout_editor_absoluteX="70dp"
            tools:layout_editor_absoluteY="78dp" />

    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="-20dp"
        android:layout_marginTop="-20dp"
        android:layout_weight="1"
        android:orientation="horizontal">

        <ImageView
            android:id="@+id/clientString"
            android:layout_width="128dp"
            android:layout_height="60dp"
            android:layout_gravity="top"
            android:layout_marginLeft="50dp"
            android:layout_marginRight="100dp"
            app:srcCompat="@drawable/clients"
            tools:layout_editor_absoluteX="204dp"
            tools:layout_editor_absoluteY="198dp" />

        <ImageView
            android:id="@+id/branchString"
            android:layout_width="138dp"
            android:layout_height="60dp"
            android:layout_gravity="top|right"
            android:layout_marginRight="50dp"
            app:srcCompat="@drawable/branches"
            tools:layout_editor_absoluteX="55dp"
            tools:layout_editor_absoluteY="206dp" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="50dp"
        android:layout_marginTop="-300dp"
        android:orientation="horizontal">

        <ImageButton
            android:id="@+id/carButton"
            android:layout_width="144dp"
            android:layout_height="105dp"
            android:layout_marginLeft="-20dp"
            android:layout_marginRight="80dp"
            android:background="#00000000"
            android:src="@drawable/carsb"
            tools:layout_editor_absoluteX="204dp"
            tools:layout_editor_absoluteY="277dp" />

        <ImageButton
            android:id="@+id/carModelButton"
            android:layout_width="100dp"
            android:layout_height="100dp"
            android:layout_marginLeft="-10dp"
            android:background="#00000000"
            android:src="@drawable/carmodelsb"
            tools:layout_editor_absoluteX="70dp"
            tools:layout_editor_absoluteY="261dp" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="35dp"
        android:orientation="horizontal">

        <ImageView
            android:id="@+id/imageView9"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_weight="1"
            app:srcCompat="@drawable/carss"
            tools:layout_editor_absoluteX="238dp"
            tools:layout_editor_absoluteY="382dp" />

        <ImageView
            android:id="@+id/CarModelString"
            android:layout_width="141dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            app:srcCompat="@drawable/carmodels"
            tools:layout_editor_absoluteX="55dp"
            tools:layout_editor_absoluteY="355dp" />
    </LinearLayout>

</LinearLayout>
